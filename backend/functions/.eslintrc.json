{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "double"],
    "object-curly-spacing": ["error", "always"],
    "no-unused-vars": "off",
    "max-len": ["error", { "code": 120 }],
    "indent": ["error", 2],
    "comma-dangle": ["error", "only-multiline"]
  },
  "parserOptions": {
    "ecmaVersion": 12
  }
  
}
